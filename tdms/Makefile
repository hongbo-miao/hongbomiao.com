poetry-update-lock-file:
	poetry lock --no-update
poetry-install:
	poetry install
poetry-add:
	poetry add xxx
poetry-add-dev:
	poetry add xxx --group=dev
poetry-run-dev:
	poetry run poe dev

download-tdms-sample:
	curl https://github.com/Hongbo-Miao/tdms-sample/raw/main/exampleMeasurements.tdms > data/exampleMeasurements.tdms
