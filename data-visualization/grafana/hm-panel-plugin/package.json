{
  "name": "hm-panel-plugin",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --maxWorkers 4",
    "tsc": "tsc --noEmit",
    "lint:javascript": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:javascript:fix": "npm run lint --fix",
    "e2e": "grafana-e2e run",
    "e2e:update": "grafana-e2e run --update-screenshots",
    "server": "docker-compose up --build"
  },
  "dependencies": {
    "@emotion/css": "11.11.2",
    "@grafana/data": "10.4.2",
    "@grafana/runtime": "10.4.2",
    "@grafana/ui": "10.4.2",
    "@tanstack/react-query": "4.36.1",
    "axios": "1.6.8",
    "cypress": "13.8.1",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@babel/core": "7.24.5",
    "@grafana/e2e": "10.4.2",
    "@grafana/e2e-selectors": "10.4.2",
    "@grafana/eslint-config": "7.0.0",
    "@grafana/tsconfig": "1.3.0-rc1",
    "@swc/core": "1.5.2",
    "@swc/helpers": "0.5.11",
    "@swc/jest": "0.2.36",
    "@tanstack/react-query-devtools": "4.36.1",
    "@testing-library/jest-dom": "6.4.3",
    "@testing-library/react": "15.0.6",
    "@types/glob": "8.1.0",
    "@types/jest": "29.5.12",
    "@types/node": "20.12.8",
    "@typescript-eslint/eslint-plugin": "7.8.0",
    "@typescript-eslint/parser": "7.8.0",
    "ajv": "8.13.0",
    "copy-webpack-plugin": "12.0.2",
    "css-loader": "7.1.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jsdoc": "48.2.3",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-webpack-plugin": "4.1.0",
    "fork-ts-checker-webpack-plugin": "9.0.2",
    "glob": "9.3.5",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.7.0",
    "prettier": "3.2.5",
    "replace-in-file-webpack-plugin": "1.0.6",
    "sass": "1.76.0",
    "sass-loader": "14.2.1",
    "style-loader": "4.0.0",
    "swc-loader": "0.2.6",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.4.5",
    "webpack": "5.91.0",
    "webpack-cli": "5.1.4",
    "webpack-livereload-plugin": "3.0.2"
  }
}
