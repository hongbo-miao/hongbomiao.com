    "eslint-plugin-security": "1.7.1",
    "eslint-plugin-testing-library": "5.10.2",
    "jest": "29.4.3",
    "prettier": "2.8.4",
    "purgecss": "5.0.0",
    "react-test-renderer": "18.2.0",
    "redux-mock-store": "1.5.4",
    "serve": "14.2.0",
