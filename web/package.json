    "eslint-plugin-testing-library": "6.4.0",
    "jest": "29.7.0",
    "prettier": "3.3.3",
    "purgecss": "6.0.0",
    "react-test-renderer": "18.3.1",
    "redux-mock-store": "1.5.5",
    "serve": "14.2.4",
