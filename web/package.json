{
  "name": "hm-web",
  "private": true,
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  },
  "scripts": {
    "dev": "react-scripts start",
    "prebuild": "sh ./bin/prebuild.sh",
    "build": "react-scripts build",
    "postbuild": "sh ./bin/postbuild.sh",
    "serve": "serve build --listen 3000",
    "analyze": "react-scripts build && source-map-explorer 'build/static/js/*.js'",
    "tsc": "tsc --noEmit",
    "test": "react-scripts test --watchAll=false",
    "test:watch": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "storybook": "start-storybook --static-dir public --port 9009",
    "storybook:build": "build-storybook -s public",
    "storybook:publish": "chromatic --build-script-name storybook:build",
    "lighthouse": "lhci autorun",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      ".query.ts",
      ".story.tsx",
      ".type.ts",
      "src/shared/libs/*",
      "src/shared/utils/initSentry.ts"
    ]
  },
  "dependencies": {
    "@analytics/google-tag-manager": "0.5.2",
    "@opentelemetry/api": "1.0.3",
    "@opentelemetry/exporter-collector": "0.25.0",
    "@opentelemetry/instrumentation": "0.26.0",
    "@opentelemetry/instrumentation-document-load": "0.25.0",
    "@opentelemetry/instrumentation-xml-http-request": "0.26.0",
    "@opentelemetry/resources": "1.0.0",
    "@opentelemetry/semantic-conventions": "1.0.0",
    "@opentelemetry/sdk-trace-base": "1.0.0",
    "@opentelemetry/sdk-trace-web": "1.0.0",
    "@sentry/react": "6.19.6",
    "analytics": "0.8.1",
    "axios": "0.26.1",
    "bulma": "0.9.3",
    "clsx": "1.1.1",
    "graphql": "16.3.0",
    "graphql-tag": "2.12.6",
    "graphql-ws": "5.6.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.22.5",
    "react-query": "3.34.19",
    "react-redux": "7.2.8",
    "react-router-dom": "6.3.0",
    "react-scripts": "4.0.3",
    "react-use": "17.3.2",
    "redux": "4.1.2",
    "redux-devtools-extension": "2.13.9",
    "redux-observable": "2.0.0",
    "rxjs": "7.5.5",
    "web-vitals": "2.1.4",
    "workbox-background-sync": "6.5.2",
    "workbox-broadcast-update": "6.5.2",
    "workbox-cacheable-response": "6.5.2",
    "workbox-core": "6.5.2",
    "workbox-expiration": "6.5.2",
    "workbox-google-analytics": "6.5.2",
    "workbox-navigation-preload": "6.5.2",
    "workbox-precaching": "6.5.2",
    "workbox-range-requests": "6.5.2",
    "workbox-routing": "6.5.2",
    "workbox-strategies": "6.5.2",
    "workbox-streams": "6.5.2"
  },
  "devDependencies": {
    "@babel/cli": "7.17.6",
    "@babel/plugin-transform-react-display-name": "7.16.7",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@lhci/cli": "0.9.0",
    "@storybook/addon-actions": "6.4.20",
    "@storybook/addon-links": "6.4.20",
    "@storybook/addons": "6.4.20",
    "@storybook/preset-create-react-app": "3.2.0",
    "@storybook/react": "6.4.20",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "12.1.4",
    "@testing-library/react-hooks": "7.0.2",
    "@testing-library/user-event": "14.0.4",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.26",
    "@types/react": "17.0.44",
    "@types/react-dom": "17.0.15",
    "@types/redux-mock-store": "1.0.3",
    "@types/storybook__react": "5.2.1",
    "chromatic": "6.5.3",
    "purgecss": "4.1.3",
    "react-test-renderer": "17.0.2",
    "redux-mock-store": "1.5.4",
    "serve": "13.0.2",
    "sitemap": "7.1.1",
    "source-map-explorer": "2.5.2",
    "typescript": "4.6.3"
  }
}
