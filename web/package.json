    "eslint-plugin-security": "1.7.1",
    "eslint-plugin-testing-library": "6.0.1",
    "jest": "29.6.4",
    "prettier": "3.0.2",
    "purgecss": "5.0.0",
    "react-test-renderer": "18.2.0",
    "redux-mock-store": "1.5.4",
    "serve": "14.2.1",
    "sitemap": "7.1.1",
