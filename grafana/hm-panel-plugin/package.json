{
  "name": "hm-panel-plugin",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "18.x",
    "npm": "8.x"
  },
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --maxWorkers 4",
    "tsc": "tsc --noEmit",
    "lint:javascript": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:javascript:fix": "npm run lint --fix",
    "e2e": "grafana-e2e run",
    "e2e:update": "grafana-e2e run --update-screenshots",
    "server": "docker-compose up --build"
  },
  "dependencies": {
    "@emotion/css": "11.10.5",
    "@grafana/data": "9.3.2",
    "@grafana/runtime": "9.3.2",
    "@grafana/ui": "9.3.2",
    "@tanstack/react-query": "4.22.0",
    "axios": "1.2.3",
    "cypress": "9.7.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@grafana/e2e": "9.3.2",
    "@grafana/e2e-selectors": "9.3.2",
    "@grafana/eslint-config": "5.1.0",
    "@grafana/tsconfig": "1.2.0-rc1",
    "@swc/core": "1.3.27",
    "@swc/helpers": "0.4.14",
    "@swc/jest": "0.2.24",
    "@tanstack/react-query-devtools": "4.22.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/glob": "8.0.0",
    "@types/jest": "29.2.6",
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "ajv": "8.12.0",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.7.3",
    "eslint": "8.32.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-jsdoc": "39.6.6",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-webpack-plugin": "3.2.0",
    "fork-ts-checker-webpack-plugin": "7.3.0",
    "glob": "8.1.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.3.1",
    "prettier": "2.8.3",
    "replace-in-file-webpack-plugin": "1.0.6",
    "sass": "1.57.1",
    "sass-loader": "13.2.0",
    "style-loader": "3.3.1",
    "swc-loader": "0.2.3",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.2",
    "typescript": "4.9.4",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1",
    "webpack-livereload-plugin": "3.0.2"
  }
}
