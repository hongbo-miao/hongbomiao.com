    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-testing-library": "5.0.1",
    "husky": "7.0.4",
    "jest": "27.4.4",
