  "private": true,
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  },
  "scripts": {
    "commit": "commit",
    "setup": "sh ./bin/setup.sh",
    "clean": "sh ./bin/clean.sh",
    "build": "sh ./bin/build.sh",
    "start": "cd api-node && npm run serve",
    "lint:js": "eslint . --ext .js,.ts,.tsx",
    "lint:js:report": "eslint-nibble . --ext .js,.ts,.tsx",
    "lint:js:fix": "eslint . --ext .js,.ts,.tsx --fix",
    "lint:css": "stylelint **/*.css",
    "lint:css:fix": "stylelint **/*.css --fix",
    "lint:sol": "solhint **/*.sol",
    "lint:sol:fix": "solhint **/*.sol --fix",
    "lint:md": "markdownlint-cli2 $(git ls-files '**/*.md')",
    "lint:md:fix": "markdownlint-cli2-fix $(git ls-files '**/*.md')",
    "test": "cd web && npm test && cd ../mobile && npm test && cd ../api-node && npm test",
    "test:coverage": "sh bin/testCoverage.sh",
    "test:coverage:publish": "cat coverage/lcov.info | codecov"
  },
  "devDependencies": {
    "@commitlint/cli": "15.0.0",
