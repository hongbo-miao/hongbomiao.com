    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-testing-library": "4.6.0",
    "husky": "6.0.0",
    "jest": "27.0.4",
    "lint-staged": "11.0.0",
