{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "hm-api-node",
  "private": true,
  "engines": {
    "node": "22.x"
  },
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "tsc && swc src --out-dir build/ --extensions=.ts && cp -R public/ build/public/",
    "build-scripts": "tsc && swc scripts --out-dir build/scripts --extensions .ts",
    "build-web": "bash scripts/build_web.sh",
    "debug": "npm run build && NODE_ENV=development node --inspect-brk build/src/index.js",
    "dev": "nodemon --ext ts --exec 'npm run build && NODE_ENV=development node build/src/index.js -- --hideHttpLog --prettifyLog'",
    "serve": "NODE_ENV=production node build/src/index.js",
    "lint-javascript": "eslint .",
    "lint-javascript-fix": "eslint . --fix",
    "static-type-check-typescript": "tsc",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "test-watch": "NODE_OPTIONS='--experimental-vm-modules' jest --watch",
    "test-coverage": "NODE_OPTIONS='--experimental-vm-modules' jest --coverage",
    "test-stryker": "NODE_OPTIONS='--experimental-vm-modules' stryker run",
    "autocannon": "autocannon http://localhost:58136",
    "profile-bubbleprof": "npm run build-scripts && NODE_ENV=production clinic bubbleprof --on-port 'bash scripts/profile.sh' -- node build/src/index.js",
    "profile-doctor": "npm run build-scripts && NODE_ENV=production clinic doctor --on-port 'bash scripts/profile.sh' -- node build/src/index.js",
    "profile-flame": "npm run build-scripts && NODE_ENV=production clinic flame --on-port 'bash scripts/profile.sh' -- node build/src/index.js"
  },
  "dependencies": {
    "@faker-js/faker": "10.1.0",
    "@gfx687/express-request-id": "1.0.3",
    "@godaddy/terminus": "4.12.1",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/exporter-trace-otlp-http": "0.207.0",
    "@opentelemetry/instrumentation": "0.207.0",
    "@opentelemetry/instrumentation-dns": "0.51.0",
    "@opentelemetry/instrumentation-express": "0.56.0",
    "@opentelemetry/instrumentation-graphql": "0.55.0",
    "@opentelemetry/instrumentation-http": "0.207.0",
    "@opentelemetry/instrumentation-ioredis": "0.55.0",
    "@opentelemetry/instrumentation-pg": "0.60.0",
    "@opentelemetry/instrumentation-pino": "0.54.0",
    "@opentelemetry/resources": "2.2.0",
    "@opentelemetry/sdk-metrics": "2.2.0",
    "@opentelemetry/sdk-trace-base": "2.2.0",
    "@opentelemetry/sdk-trace-node": "2.2.0",
    "@opentelemetry/semantic-conventions": "1.37.0",
    "@sentry/node": "10.22.0",
    "@typescript-eslint/eslint-plugin": "8.46.2",
    "@typescript-eslint/parser": "8.46.2",
    "bcrypt": "6.0.0",
    "connect-timeout": "1.9.1",
    "cookie-parser": "1.4.7",
    "cors": "2.8.5",
    "dataloader": "2.2.3",
    "dotenv-flow": "4.1.0",
    "express": "5.1.0",
    "express-jwt": "8.5.1",
    "graphql": "16.11.0",
    "graphql-depth-limit": "1.1.0",
    "graphql-http": "1.22.4",
    "graphql-middleware": "6.1.35",
    "graphql-shield": "7.6.5",
    "graphql-subscriptions": "3.0.0",
    "graphql-upload": "17.0.0",
    "graphql-ws": "6.0.6",
