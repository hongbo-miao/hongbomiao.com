    "pino": "10.1.0",
    "pino-http": "11.0.0",
    "rate-limiter-flexible": "8.1.0",
    "report-to": "1.1.0",
    "request-ip": "3.3.0",
    "response-time": "2.3.4",
    "serve-favicon": "2.5.1",
    "spdy": "4.0.2",
    "validator": "13.15.20",
    "ws": "8.18.3",
    "yargs": "18.0.0"
