{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "hm-api-node",
  "private": true,
  "engines": {
    "node": "22.x"
  },
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "tsc && swc src --out-dir build/ --extensions=.ts && cp -R public/ build/public/",
    "build:scripts": "tsc && swc scripts --out-dir build/scripts --extensions .ts",
    "build:web": "bash bin/build_web.sh",
    "debug": "npm run build && NODE_ENV=development node --inspect-brk build/src/index.js",
    "dev": "nodemon --ext ts --exec 'npm run build && NODE_ENV=development node build/src/index.js -- --hideHTTPLog --prettifyLog'",
    "serve": "NODE_ENV=production node build/src/index.js",
    "tsc": "tsc",
    "lint:javascript": "eslint .",
    "lint:javascript:fix": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:stryker": "stryker run",
    "autocannon": "autocannon http://localhost:58136",
    "profile:endpoints": "npm run build:scripts && NODE_ENV=production node build/scripts/profileEndpoints.js",
    "profile:bubbleprof": "npm run build:scripts && NODE_ENV=production clinic bubbleprof --on-port 'bash bin/profile.sh' -- node build/src/index.js",
    "profile:doctor": "npm run build:scripts && NODE_ENV=production clinic doctor --on-port 'bash bin/profile.sh' -- node build/src/index.js",
    "profile:flame": "npm run build:scripts && NODE_ENV=production clinic flame --on-port 'bash bin/profile.sh' -- node build/src/index.js"
  },
  "dependencies": {
    "@faker-js/faker": "9.9.0",
    "@gfx687/express-request-id": "1.0.3",
    "@godaddy/terminus": "4.12.1",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/exporter-trace-otlp-http": "0.203.0",
    "@opentelemetry/instrumentation": "0.203.0",
    "@opentelemetry/instrumentation-dns": "0.47.0",
    "@opentelemetry/instrumentation-express": "0.52.0",
    "@opentelemetry/instrumentation-graphql": "0.51.0",
